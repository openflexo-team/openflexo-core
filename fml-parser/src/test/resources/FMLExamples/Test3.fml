use org.openflexo.technologyadapter.owl.OWLTechnologyAdapter as OWL;

import http://www.openflexo.org/Tests/ViewPoints/ViewPoint1 as ViewPoint1;
import http://www.openflexo.org/Tests/OWL/Ontology1.owl as Ontology1;

@Version("1.1")
@Author("Sylvain")
@Description("This is a description of VirtualModel")

@URI("http://openflexo.org/test/Tes1VM.fml"
public VirtualModel Test1VM extends Test2VM,ViewPoint1.AnOtherVM {

	@Description("This model slot points to an ontology, which import here ontology with uri Ontology1")
	OWL::OWLModelSlot<OWLOntology> ontology (importOntology=FML::Resource<OWLOntology>(uri=Ontology1));
	
	@Version("1.2")
	@Description("This is the first concept")
	public FlexoConcept Concept1 {
		
		public PrimitiveRole<int> v1;
		public PrimitiveRole<int> v2;
		public Expression<int> sum = v1+v2;
		public GetSet<int> c (
			get: {
				return v1+v2:
			}
			set: (int v) {
				v1 = v;
				v2 = 0;
			}
		)
		create:init(int aV1, int aV2) {
			v1 = aV1;
			v2 = aV2;
		}
		render: {
			return “Toto”+v1+v2;
		}
		void action:helloWorld() {
			print(“Hello World !”);
		}
	}
	

}
