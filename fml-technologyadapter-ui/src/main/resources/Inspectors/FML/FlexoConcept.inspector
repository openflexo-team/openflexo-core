						action="data.moveInvariantToIndex(invariantsTable.selected,invariantsTable.selected.index+1)" index="2"
						localize="true"
						constraints="box(alignmentX=0.5;alignmentY=0.5;index=2;location=center)"
						label="">
					</Button>
					<Button
