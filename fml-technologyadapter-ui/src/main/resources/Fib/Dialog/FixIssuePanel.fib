			flowAlignment="LEADING" borderLeft="0" trackViewPortHeight="true"
			trackViewPortWidth="true" borderBottom="0" border="empty"
			sizeAdjustment="OriginalSize" layout="flow" borderRight="0" vGap="0"
			hGap="0"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=5;insetsRight=0;insetsTop=0;location=right)"
			name="FIBPanel2">
			<Image id="16" imageWidth="80" sizeAdjustment="OriginalSize"
				align="left" imageHeight="46"
				data="controller.iconForObject(data.issue.validable).image"
				constraints="flow()" name="Image2" />
			<Label id="17" label="label" align="left"
				data="data.issue.validable.toString" constraints="flow()"
				name="Label6" />
		</Panel>
		<Label id="18" label="rule" align="left" localize="true"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=5;location=left)"
			name="Label2" />
		<TextField id="19" columns="7" readOnly="true"
			data="data.validationModel.localizedRuleName(data.issue.cause)"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=5;location=right)"
			name="TextField" />
		<Label id="20" label="details" align="left" localize="true"
			visible="(data.issue.detailedInformations != null)"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=5;location=left)"
			name="Label5" />
		<TextArea id="21" rows="5" columns="8"
			validateOnReturn="true" text="TextArea" readOnly="true"
			data="data.validationModel.localizedIssueDetailedInformations(data.issue)"
			useScrollBar="true"
			visible="(data.issue.detailedInformations != null)"
			verticalScrollbarPolicy="VERTICAL_SCROLLBAR_AS_NEEDED"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=5;location=right)"
			horizontalScrollbarPolicy="HORIZONTAL_SCROLLBAR_AS_NEEDED"
			font="SansSerif,0,10" name="TextArea" />
		<Label id="22" label="available fixes" align="left"
			localize="true"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=10;location=left)"
			name="Label7" />
		<Table id="23" visibleRowCount="3"
			boundToSelectionManager="true"
			backgroundNonSelectionColor="255,255,255"
			iteratorClassName="org.openflexo.pamela.validation.FixProposal&lt;? extends org.openflexo.pamela.validation.ValidationRule&lt;? extends org.openflexo.pamela.validation.ValidationRule&lt;?,?&gt;,? extends org.openflexo.pamela.validation.Validable&gt;,? extends org.openflexo.pamela.validation.Validable&gt;"
			backgroundSecondarySelectionColor="178,215,255" showFooter="false"
			autoSelectFirstRow="true" lookAndFeel="Classic"
			selectionMode="MultipleIntervalSelection" selected="data.fixProposal"
			rowHeight="18" localize="true" manageDynamicModel="true"
			data="data.issue.fixProposals"
			constraints="twocols(expandHorizontally=true;expandVertically=true;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=10;location=right)"
			name="FixProposalsTable">
			<IconColumn id="24"
				data="controller.iconForObject(iterator)" resizable="false"
				title="icon" displayTitle="false" columnWidth="20" showIcon="false"
				name="icon" />
			<LabelColumn id="25"
				data="data.validationModel.localizedFixProposal(iterator)"
				resizable="true" title="fix_proposals" displayTitle="true"
				columnWidth="100" showIcon="false" name="label" />
		</Table>
