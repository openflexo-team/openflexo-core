<?xml version="1.0" encoding="UTF-8"?>
<Panel id="0" align="left" borderTop="0" borderLeft="0"
	trackViewPortHeight="true" trackViewPortWidth="true" borderBottom="0"
	border="empty" sizeAdjustment="OriginalSize" layout="border"
	borderRight="0"
	controllerClassName="org.openflexo.fml.controller.FMLFIBController"
	opaque="true" name="FixIssuePanel">
	<Panel id="1" align="left" borderTop="10" borderLeft="10"
		trackViewPortHeight="true" trackViewPortWidth="true" borderBottom="10"
		border="empty" sizeAdjustment="OriginalSize" layout="border"
		borderRight="10" backgroundColor="255,255,255" opaque="true"
		constraints="border(location=north)" name="Header">
		<Image id="2" imageWidth="64" sizeAdjustment="OriginalSize"
			imageFile="../../Icons/Validation/FixIssue_64x64.png" align="left"
			imageHeight="64" localize="true" constraints="border(location=east)"
			name="Image" />
		<Panel id="3" align="left" borderTop="0" borderLeft="0"
			trackViewPortHeight="true" trackViewPortWidth="true" borderBottom="0"
			border="empty" sizeAdjustment="OriginalSize" layout="border"
			borderRight="0" constraints="border(location=center)"
			name="FIBPanel3">
			<Label id="4" label="fix_issue" align="left" localize="true"
				constraints="border(location=north)" font="SansSerif,1,16"
				name="Label" />
			<Panel id="5" align="left" borderTop="0" flowAlignment="LEFT"
				borderLeft="0" trackViewPortHeight="true" trackViewPortWidth="true"
				borderBottom="0" border="empty" sizeAdjustment="OriginalSize"
				layout="flow" borderRight="0" vGap="0" hGap="0"
				backgroundColor="255,255,255" opaque="true"
				constraints="border(location=south)" name="FIBPanel">
				<Image id="6" imageWidth="16" sizeAdjustment="OriginalSize"
					align="right" imageHeight="16" localize="true"
					data="controller.iconForObject(data.issue).image"
					constraints="flow()" name="Image" />
				<Label id="7" label="label" align="left"
					data="data.validationModel.localizedIssueMessage(data.issue)"
					constraints="flow()" font="SansSerif,0,11" name="Label" />
			</Panel>
		</Panel>
	</Panel>
	<Panel id="8" align="left" borderTop="5" flowAlignment="RIGHT"
		borderLeft="10" trackViewPortHeight="true" trackViewPortWidth="true"
		borderBottom="5" border="empty" sizeAdjustment="OriginalSize"
		layout="flow" borderRight="10" vGap="5" hGap="5"
		constraints="border(location=south)" name="ControlPanel">
		<Button id="9" buttonType="Trigger"
			action="controller.cancelAndDispose()" label="cancel" localize="true"
			enable="" constraints="flow()" name="NextButton" />
		<Button id="10" buttonType="Trigger"
			action="controller.chooseNoAndDispose()" label="ignore"
			localize="true" enable="data.isIgnorable" constraints="flow()"
			name="CancelButton" />
		<Button id="11" buttonType="Trigger"
			action="controller.validateAndDispose()" label="fix" localize="true"
			enable="data.isFixable" constraints="flow()" name="FinishButton" />
	</Panel>
	<Panel id="12" align="left" borderTop="10" borderLeft="10"
		trackViewPortHeight="true" trackViewPortWidth="true" borderBottom="2"
		border="empty" sizeAdjustment="OriginalSize" darkLevel="1"
		layout="twocols" borderRight="10"
		constraints="border(location=center)" name="ContentsPanel">
		<Label id="13" label="label" align="center"
			data="data.validationModel.localizedRuleDescription(data.issue.cause)"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=20;insetsLeft=0;insetsRight=0;insetsTop=20;location=center)"
			font="SansSerif,2,12" name="Label3" />
		<Label id="14" label="object" align="left" localize="true"
			constraints="twocols(expandHorizontally=true;expandVertically=false;insetsBottom=0;insetsLeft=0;insetsRight=0;insetsTop=0;location=left)"
			name="Label4" />
