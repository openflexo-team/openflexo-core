import java.lang.String;

@URI("http://openflexo.org/test/TestResourceCenter/TestSingleInheritance.fml")
@Version("0.1")
@Author("sylvain")
model TestSingleInheritance {

	@Author("sylvain")
	concept A {
		String foo;
		create::init () {
			log("Creating A instance");
			foo = "A";
		}
		delete::_delete () {
		}
		doSomething () {
			log("Do something in A");
			return 42;
		}
	}

	@Author("sylvain")
	concept B extends A {
		create::init () {
			super.init();
			log("Creating B instance");
			foo = (foo + "B");
		}
		delete::_delete () {
		}
		doSomething () {
			log("Do something in B");
			return (super.doSomething() * 2);
		}
	}
}